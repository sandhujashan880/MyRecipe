<h1 class="headingofrec"> <%= render 'shared/page_title', title: 'ALL Chefs' %> </h1>

<div class="row">
  <div class="col-md-4 col-md-offset-4 cneter">
    <%= will_paginate %>
  </div>
</div>

<% @chefs.each do |chef| %>
<div class="row">
  <div class="col-md-4 col-md-offset-4 cneter">
    <%= link_to gravatar_for(chef,size: 200), chef_path(chef) %>
    <p>
      <h2> Chefname: <%= chef.chefname %></h2>
    </p>
    
  </div>
</div>


<% end %>